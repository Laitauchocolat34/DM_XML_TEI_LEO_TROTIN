         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-p-abstractModel-structure-p-in-l-or-lg-constraint-report-6">
            <rule context="tei:p">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
